Create table records
(
	userID int Primary key,
	userName text,
	userSurname text,
	userCountry text,
	userCity text,
	userPhone text,
	photoLink text	
)
Create table friendsRecords
(
	recordID int not null Primary key GENERATED BY DEFAULT AS IDENTITY,
	ownerID int not null,
	friendID int not null,
	friendName text,
	FOREIGN KEY (ownerID) REFERENCES records (userID)
)
Create table videosRecords
(
	recordID int not null Primary key GENERATED BY DEFAULT AS IDENTITY,
	ownerID int not null,
	mainOwnerID int,
	videoID text,
	videoName text,
	videoLink text,
	FOREIGN KEY (ownerID)Â REFERENCES records (userID)
)
Create table groupsRecords
(
	recordID int not null Primary key GENERATED BY DEFAULT AS IDENTITY,
	ownerID int not null,
	groupID int not null,
	groupName text,
	FOREIGN KEY (ownerID) REFERENCES records (userID)
)
